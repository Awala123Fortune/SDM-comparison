			ng[which(betavAll[,,i]<0,arr.ind=T)]<- -1#
			Pos<-Pos+pos#
			Ng<-Ng+ng#
		}#
		supportPos<-abs(Pos)/(qvMeans*dim(betavAll)[3])#
